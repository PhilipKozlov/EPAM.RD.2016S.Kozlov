<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="ServiceConfig" type="SystemConfigurator.ServiceConfigSection, SystemConfigurator"/>   
  </configSections>
  <ServiceConfig>
    <Services>
      <Service id="1" role="Master" type="UserStorage.IUserService, UserStorage" port="1111" host="127.0.0.1"/>
      <Service id="2" role="Slave" type="UserStorage.IUserService, UserStorage" port="2222" host="127.0.0.1"/>
      <Service id="3" role="Slave" type="UserStorage.IUserService, UserStorage" port="3333" host="127.0.0.1"/>
      <Service id="4" role="Slave" type="UserStorage.IUserService, UserStorage" port="4444" host="127.0.0.1"/>
    </Services>
  </ServiceConfig>
    
  <system.diagnostics>
    <switches>
        <add name="logSwitch" value="0"/>
    </switches>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="Log.log" />
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <appSettings>
    <add key="Path" value="users.xml" />
  </appSettings>
  
</configuration>